---
kind: Command
version: v1
definition:
  name: EmployeeAggregate
  outputType: EmployeeAggregate!
  arguments:
    - name: distinctOn
      type: "[EmployeeSelectColumn!]"
      description: distinct select on columns
    - name: limit
      type: Int
      description: limit the number of rows returned
    - name: offset
      type: Int
      description: skip the first n rows. Use only with order_by
    - name: orderBy
      type: "[EmployeeOrderBy!]"
      description: sort the rows by one or more columns
    - name: where
      type: EmployeeBoolExp
      description: filter the rows returned
  source:
    dataConnectorName: my_graphql
    dataConnectorCommand:
      function: Employee_aggregate
    argumentMapping:
      distinctOn: distinct_on
      limit: limit
      offset: offset
      orderBy: order_by
      where: where
  graphql:
    rootFieldName: employeeAggregate
    rootFieldKind: Query
  description: 'fetch aggregated fields from the table: "Employee"'

