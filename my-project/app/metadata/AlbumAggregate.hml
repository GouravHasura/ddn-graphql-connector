---
kind: Command
version: v1
definition:
  name: AlbumAggregate
  outputType: AlbumAggregate!
  arguments:
    - name: distinctOn
      type: "[AlbumSelectColumn!]"
      description: distinct select on columns
    - name: limit
      type: Int
      description: limit the number of rows returned
    - name: offset
      type: Int
      description: skip the first n rows. Use only with order_by
    - name: orderBy
      type: "[AlbumOrderBy!]"
      description: sort the rows by one or more columns
    - name: where
      type: AlbumBoolExp
      description: filter the rows returned
  source:
    dataConnectorName: my_graphql
    dataConnectorCommand:
      function: Album_aggregate
    argumentMapping:
      distinctOn: distinct_on
      limit: limit
      offset: offset
      orderBy: order_by
      where: where
  graphql:
    rootFieldName: albumAggregate
    rootFieldKind: Query
  description: 'fetch aggregated fields from the table: "Album"'

