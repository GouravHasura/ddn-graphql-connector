kind: ObjectType
version: v1
definition:
  name: AlbumAggregateFields
  description: aggregate fields of "Album"
  fields:
    - name: avg
      type: AlbumAvgFields
    - name: count
      type: Int!
      arguments:
        - name: columns
          argumentType: "[AlbumSelectColumn!]"
        - name: distinct
          argumentType: Boolean
    - name: max
      type: AlbumMaxFields
    - name: min
      type: AlbumMinFields
    - name: stddev
      type: AlbumStddevFields
    - name: stddevPop
      type: AlbumStddevPopFields
    - name: stddevSamp
      type: AlbumStddevSampFields
    - name: sum
      type: AlbumSumFields
    - name: varPop
      type: AlbumVarPopFields
    - name: varSamp
      type: AlbumVarSampFields
    - name: variance
      type: AlbumVarianceFields
  graphql:
    typeName: AlbumAggregateFields
    inputTypeName: AlbumAggregateFieldsInput
  dataConnectorTypeMapping:
    - dataConnectorName: my_graphql
      dataConnectorObjectType: Album_aggregate_fields
      fieldMapping:
        avg:
          column:
            name: avg
        count:
          column:
            name: count
        max:
          column:
            name: max
        min:
          column:
            name: min
        stddev:
          column:
            name: stddev
        stddevPop:
          column:
            name: stddev_pop
        stddevSamp:
          column:
            name: stddev_samp
        sum:
          column:
            name: sum
        varPop:
          column:
            name: var_pop
        varSamp:
          column:
            name: var_samp
        variance:
          column:
            name: variance

---
kind: TypePermissions
version: v1
definition:
  typeName: AlbumAggregateFields
  permissions:
    - role: admin
      output:
        allowedFields:
          - avg
          - count
          - max
          - min
          - stddev
          - stddevPop
          - stddevSamp
          - sum
          - varPop
          - varSamp
          - variance

